{
    "name": "fateme-mahmoodi/laravel_todo",
    "description": "laravel todo is a package that can use for todo list on your project",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "7.4",
        "laravel/framework": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "FatemeMahmoodi\\LaravelToDo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FatemeMahmoodi\\LaravelToDo\\Tests\\": "tests/",
            "FatemeMahmoodi\\LaravelToDo\\Database\\Factories\\": "database/factories/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "FatemeMahmoodi\\LaravelToDo\\LaravelTodoServiceProvider"
            ]
        }
    },

    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0"
    }
}
